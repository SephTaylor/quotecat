{
  "extends": "expo/tsconfig.base",
"compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/modules/*": ["modules/*"],
      "@/lib/*": ["lib/*"],
      "@/constants/*": ["constants/*"]
    },
    "strict": true
  },
  "include": ["app/**/*", "lib/**/*", "modules/**/*", "scripts/**/*", "expo-env.d.ts", ".expo/types/**/*.ts"],
  "exclude": ["_old", "node_modules", "app/_old", "modules/_old"]
}
