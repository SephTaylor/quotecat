{
  "expo": {
    "name": "QuoteCat",
    "slug": "quotecat",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quotecat",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://quotecat.ai"
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/qc-splash-tsnt-2048-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "ai.quotecat.app",
      "icon": "./assets/images/icon.png",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/qc-splash-tsnt-2048-text.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:quotecat.ai"
      ]
    },
    "android": {
      "package": "ai.quotecat.app",
      "icon": "./assets/images/icon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "quotecat.ai",
              "pathPrefix": "/auth/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dc5517f1-c28a-4507-804b-406cee16ff3c"
      }
    }
  }
}
